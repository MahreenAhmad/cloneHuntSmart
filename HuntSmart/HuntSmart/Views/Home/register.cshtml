    <script>
        $(document).ready(function () {
			//for search button redirection

      $('#search-button').click(function () {
        
        location.href = "/Home/companylist";
        
      });
		
            //alert("on ready called");
            $("#dialog").dialog({
                autoOpen: false,
                modal: true,
                resizable: true
            });
            $("#forgetEmail").on("click", function () {
                $("#dialog").dialog("open");
            });
            //$("#forgetEmail").on("click", function () {
            //    alert("-_-");
            //    });

            // Validating Form Fields.....
            $("#submit").click(function (e) {
                var email = $("#email").val();
                var name = $("#name").val();
                var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
                if (email === '' || name === '') {
                    alert("Please fill all fields...!!!!!!");
                    e.preventDefault();
                } else if (!(email).match(emailReg)) {
                    alert("Invalid Email...!!!!!!");
                    e.preventDefault();
                } else {
                    alert("Form Submitted Successfully......");
                }
            });
            console.log("ready!");
            $("#loginButton").click(function () {
                var email = $('#email').val();
                var pass = $('#password').val();
                if (email == "a@a.com" && pass == "123") {
                    window.location.href = "/Home/index_l";
                }
                else {
                    //alert("no correct info");
                    $('#errorMessage').text("In Correct Info,Please Try Again");
                    $('#errorMessage').css({ "font-size": "120%", "color": "red" });
                }
            });
        });
    </script>
